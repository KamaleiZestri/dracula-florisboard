{
    "$schema": "https://schemas.florisboard.org/snygg/v2/stylesheet",
    "window": {
        "background": "var(--background)"
    },
    "key": {
        "background": "var(--surface)",
        "foreground": "var(--on-surface)",
        "font-size": "22sp",
        "shadow-elevation": "var(--shadow)",
        "shape": "var(--shape)"
    },
    "key[code=10]": {
        "background": "var(--primary)",
        "foreground": "var(--on-surface)"
    },
    "key[code=32]": {
        "background": "var(--surface)",
        "foreground": "var(--on-surface-variant)",
        "font-size": "12sp"
    },
    "key[code=-202]": {
        "font-size" : "20sp"

    },
    "key[code=-201,-205]": {
        "font-size" : "16sp",
        "margin" : "0dp 0dp 0dp 0dp"

    },
    "key:pressed": {
        "background": "var(--surface-variant)",
        "foreground": "var(--on-surface)"
    },
    "key[code=10]:pressed": {
        "background": "var(--primary-variant)",
        "foreground": "var(--on-surface)"
    },
    "key-hint": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-surface-variant)",
        "font-size": "12sp",
        "margin": "0dp 1dp 4dp 0dp"
    },
    "key-popup-box": {
        "background": "var(--primary)",
        "foreground": "var(--on-surface)",
        "font-size": "22sp",
        "shadow-elevation": "var(--shadow)",
        "shape": "var(--shape)"
    },
    "key-popup-element": {
        "shape": "var(--shape)"
    },
    "key-popup-element:focus": {
        "background": "var(--primary-variant)",
        "foreground": "var(--on-surface)"
    },
    "smartbar-primary-actions-toggle": {
        "background": "var(--surface)",
        "foreground": "var(--on-surface)",
        "shadow-elevation": "var(--shadow)",
        "shape": "circle()"
    },
    "smartbar-shared-actions-toggle": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-surface-variant)",
        "shape": "circle()"
    },
    "smartbar-extended-actions-toggle": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-surface-variant)",
        "shape": "circle()"
    },
    "smartbar-quick-action": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-background)",
        "shape": "circle()"
    },
    "smartbar-action-key": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-background)",
        "font-size": "22sp",
        "shape": "var(--shape)"
    },
    "smartbar-action-key:pressed": {
        "background": "var(--surface)",
        "foreground": "var(--on-surface)"
    },
    "smartbar-action-key:disabled": {
        "background": "rgba(0,0,0,0)",
        "foreground": "rgba(220,220,220,0.28235295)"
    },
    "smartbar-candidate-word": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-background)",
        "font-size": "14sp",
        "shape": "rectangle()"
    },
    "smartbar-candidate-word:pressed": {
        "background": "var(--surface)",
        "foreground": "var(--on-background)"
    },
    "smartbar-candidate-clip": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-background)",
        "font-size": "14sp",
        "shape": "rounded-corner(8%,8%,8%,8%)"
    },
    "smartbar-candidate-clip:pressed": {
        "background": "var(--surface)",
        "foreground": "var(--on-background)"
    },
    "smartbar-candidate-spacer": {
        "foreground": "var(--surface)"
    },
    "smartbar-action-tile": {
        "foreground": "var(--on-background)",
        "font-size": "16sp"
    },
    "smartbar-actions-overflow-customize-button": {
        "foreground": "var(--background)",
        "background": "var(--on-background)"
    },
    "clipboard-header": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-surface)",
        "font-size": "16sp"
    },
    "clipboard-header-button": {
        "font-size": "22sp"
    },
    "clipboard-subheader": {
        "foreground": "var(--on-surface)",
        "padding": "10dp 4dp 10dp 4dp"
    },
    "clipboard-item": {
        "background": "var(--surface)",
        "foreground": "var(--on-surface)",
        "font-size": "14sp",
        "shadow-elevation": "var(--shadow)",
        "shape": "var(--shape-variant)",
        "margin": "8dp 8dp 8dp 8dp",
        "padding": "10dp 10dp 10dp 10dp"
    },
    "clipboard-item-popup": {
        "background": "var(--surface)",
        "foreground": "var(--on-surface)",
        "font-size": "14sp",
        "shadow-elevation": "var(--shadow)",
        "shape": "var(--shape-variant)",
        "margin": "8dp 8dp 8dp 8dp",
        "padding": "10dp 10dp 10dp 10dp"
    },
    "clipboard-item-actions": {
        "background": "var(--surface-variant)",
        "foreground": "var(--on-surface)",
        "font-size": "18sp",
        "shadow-elevation": "var(--shadow)",
        "shape": "var(--shape-variant)",
        "margin": "8dp 8dp 8dp 8dp",
        "padding": "10dp 10dp 10dp 10dp"
    },
    "clipboard-item-action": {
        "padding": "3dp 5dp 0dp 5dp"
    },
    "clipboard-item-timestamp": {
        "foreground": "var(--on-surface)",
        "padding": "10dp 4dp 10dp 4dp"
    },
    "media": {
        "foreground": "var(--on-surface)"
    },
    "media-emoji-key": {
        "background": "rgba(0,0,0,0)",
        "foreground": "var(--on-background)",
        "font-size": "22sp",
        "shape": "var(--shape)"
    },
    "media-emoji-key:pressed": {
        "background": "var(--surface)",
        "foreground": "var(--on-surface)"
    },
    "media-emoji-key-popup": {
        "background": "rgba(117,117,117,1)",
        "foreground": "var(--on-surface)",
        "font-size": "22sp",
        "shadow-elevation": "var(--shadow)",
        "shape": "var(--shape)"
    },
    "media-emoji-tab": {
        "foreground": "var(--on-background)"
    },
    "media-emoji-tab:focus": {
        "foreground": "var(--primary)"
    },
    "media-bottom-row": {
        "foreground": "var(--on-surface)",
        "margin": "10dp 0dp 10dp 0dp"
    },
    "glide-trail": {
        "foreground": "var(--primary)"
    },
    "one-handed-panel": {
        "background": "rgba(40,42,54,1)",
        "foreground": "rgba(238,238,238,1)"
    },
    "system-nav-bar": {
        "background": "var(--background)"
    }
}